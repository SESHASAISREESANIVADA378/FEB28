//prime or not
#include <stdio.h>
main() 
{
    int i,n,r,x=0,c;
    for(n=1;n<=100;n++)
	{
      x=0;
    for(i=2;i<=n/2;i++)
   {
       r=n%i;
       if(r==0)
	   {
           x=1;
         break;
        }
   }
   if(x==0)
   {
   printf("%d\t",n);
   c++;
   }
}
printf("\nprime count=%d",c);
}
